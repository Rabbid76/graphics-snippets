{
  "name": "three.js-examples",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "install:ncu": "npm install -g npm-check-updates",
    "install:webpack": "npm install --save-dev webpack",
    "update:modules": "ncu -u && npm update && npm i",
    "build:client": "webpack --config ./src/client/webpack.prod.js",
    "build:server": "tsc -p ./src/server",
    "build": "npm run build:client && npm run build:server",
    "dev": "webpack serve --config ./src/client/webpack.dev.js",
    "start": "node ./dist/server/three_server.js",
    "copy:deploy": "copyfiles --flat ./dist/client/*.js ./deployment"
  },
  "author": "Rabbid76",
  "license": "ISC",
  "devDependencies": {
    "@types/dat.gui": "^0.7.7",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@types/three": "^0.146.0",
    "copyfiles": "^2.4.1",
    "dat.gui": "^0.7.9",
    "install-peers": "^1.0.4",
    "three": "^0.146.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
